{
  "name": "hotdog-end",
  "version": "1.0.0",
  "description": "Node.js 后台项目",
  "author": "sivan.shanghai@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start:dev": "node env/setup-env.js nest start --watch",
    "start:staging": "cross-env ENVFILE=.env.staging.local node env/setup-env.js nest start --debug --watch",
    "start:prod": "cross-env ENVFILE=.env.production node env/setup-env.js node dist/main",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "eslint \"{src,test}/**/*.ts\" --color --quiet --fix",
    "lint:tsc": "tsc --noEmit",
    "prettier": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "4.5.1",
    "@nestjs/typeorm": "^7.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "4.1.4",
    "typeorm": "^0.2.25",
    "uuid": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.0.0",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "jest": "27.0.6",
    "lint-staged": "^10.2.11",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
